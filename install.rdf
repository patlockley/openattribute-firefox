<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
	    <!--
	        Extension identifier.

	        It's not likely there`d be the other project named "cc.ff_ext" hosted on the Creative Commons Code website,
	        so this identifier is considered unique
	    -->
	    <em:id>ccffext@code.creativecommons.org</em:id>

	    <!--
	        Extension version.
	    -->
	    <em:version>0.8.1</em:version>

	    <!--
	        Extension  type.

	        Here "2" means this is an extension, not a theme, etc.
	    -->
	    <em:type>2</em:type>

	    <!-- Extension name -->
	    <em:name>OpenAttribute</em:name>

	    <!-- Extension description -->
	    <em:description>Displays license and attribution information for Creative Commons licensed content.</em:description>

	    <!-- Extension contributors -->
	    <em:creator>Creative Commons (webmaster@creativecommons.org)</em:creator>
	    <em:developer>Igor Lukanin (mail@igor.lukanin.name)</em:developer>
	    <em:developer>Nathan Yergler (nathan@yergler.net)</em:developer>

	    <!--
	        Extension homepage.
	    -->
	    <em:homepageURL>http://wiki.creativecommons.org/MozCC</em:homepageURL>

	    <!-- Shows what application is augmented using this extension -->
	    <em:targetApplication>
		    <Description>
			    <!-- Firefox identifier -->
			    <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
			    <!--
			       Supported versions; see
			       https://addons.mozilla.org/en-US/firefox/pages/appversions/
			    -->
			    <em:minVersion>3.6</em:minVersion>
			    <em:maxVersion>4.0.*</em:maxVersion>
		    </Description>
	    </em:targetApplication>
	</Description>
</RDF>
